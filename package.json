{
    "name": "express-site-example",
    "version": "0.0.0",
    "private": true,
    "keywords": [
        "nodejs",
        "express",
        "mysql",
        "jwt"
    ],
    "author": "Avans ICT",
    "license": "ISC",
    "prettier": {
        "singleQuote": true,
        "trailingComma": "none",
        "semi": false,
        "tabWidth": 4
    },
    "scripts": {
        "start": "node ./bin/www",
        "dev": "nodemon ./bin/www",
        "debug": "SET DEBUG=express-site-example:* & npm run devstart",
        "test": "node ./node_modules/mocha/bin/mocha --reporter spec --exit"
    },
    "nodemonConfig": {
        "ignore": [
            "**/test/**",
            "**/docs/**"
        ],
        "ext": "js,json, pug, css",
        "delay": 2500
    },
    "dependencies": {
        "cookie-parser": "~1.4.7",
        "debug": "~4.4.0",
        "express": "^5.1.0",
        "express-async-handler": "^1.2.0",
        "morgan": "~1.10.0",
        "nodemon": "^3.1.1",
        "pug": "^3.0.3",
        "dotenv": "^16.5.0",
        "jsonwebtoken": "^9.0.2",
        "mysql2": "^3.14.1",
        "tracer": "^1.3.0"
    },
    "devDependencies": {
        "eslint": "^9.26.0",
        "chai": "^5.2.0",
        "chai-http": "^5.1.2",
        "mocha": "^11.2.2",
        "nodemon": "^3.1.10"
    }    
}
